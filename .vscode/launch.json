{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Portfolio (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: start",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Launch Portfolio (Edge)",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: start",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
